package {		import flash.display.MovieClip;	import fl.containers.*;			public class ListBoxPhoto extends MovieClip {		public var picBox:UILoader;				public function ListBoxPhoto(picBoxFoulderURL:String,picBoxpath:String){			picBox = new UILoader();			picBox.setSize(100,100);			picBox.autoLoad = true;			picBox.source = picBoxFoulderURL+picBoxpath;			this.addChild(picBox);			picBox.load();		}	}	}