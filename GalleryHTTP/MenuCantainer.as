package  {		import flash.display.MovieClip;	import fl.controls.ProgressBar;	import fl.controls.ProgressBarMode;	import fl.containers.UILoader;	import flash.events.*;	import flash.net.*;	import fl.data.DataProvider;	import flash.utils.*;	import flash.ui.*;			import gs.*;	import gs.easing.*;	import gs.events.*;	import gs.plugins.*;		public class MenuCantainer extends MovieClip {				public static var rightArrow:RightArrow;		public static var leftArrow:LeftArrow;			public function MenuCantainer() {						rightArrow = new RightArrow();			leftArrow = new LeftArrow();						this.addChild(rightArrow);			rightArrow.x = 883.00;			rightArrow.y = 63.25;			rightArrow.alpha = 0.0;						this.addChild(leftArrow);			leftArrow.x = 44.00;			leftArrow.y = 63.25;			leftArrow.alpha = 0.0;					}						public static function updateRight(rightArrowOn:Boolean){						if(rightArrowOn){				rightArrow.alpha = 1.0;			}else if(!rightArrowOn){				rightArrow.alpha = 0.0;			}								}				public static function updateLeft(leftArrowOn:Boolean){						if(leftArrowOn){				leftArrow.alpha = 1.0;			}else{				leftArrow.alpha = 0.0;			}						}	}	}